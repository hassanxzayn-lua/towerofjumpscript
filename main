-- Define the coordinates for teleportation
local x = 33.42024612426758
local y = 219.23101806640625
local z = 141.9609832763672

-- Define a variable to keep track of the teleport loop
local teleporting = false

-- Function to start teleporting
local function startTeleport()
    teleporting = true
    while teleporting do
        game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(x, y, z)
        wait(0)
    end
end

-- Function to stop teleporting
local function stopTeleport()
    teleporting = false
end

-- Create the toggle for auto teleportation
local Toggle = MainTab:CreateToggle({
    Name = "Auto Tp To Top",
    CurrentValue = false,
    Flag = "Toggle1",
    Callback = function(Value)
        if Value then
            startTeleport()
        else
            stopTeleport()
        end
    end,
})

